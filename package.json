{
    "name": "api-growdevers-9a-edicao",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn ./src/index.ts",
        "dev:debug": "ts-node-dev --inspect --respawn ./src/index.ts",
        "postinstall": "tsc",
        "typeorm": "typeorm-ts-node-commonjs",
        "migration:generate": "npm run typeorm migration:generate -- -d ./src/main/config/typeorm.config.ts",
        "migration:create": "npm run typeorm migration:create",
        "migration:run": "npm run typeorm migration:run -- -d ./src/main/config/typeorm.config.ts",
        "migration:revert": "npm run typeorm migration:revert -- -d ./src/main/config/typeorm.config.ts",
        "test": "jest --silent --noStackTrace --runInBand",
        "test:verbose": "jest --verbose --runInBand --detectOpenHandles",
        "test:coverage": "jest --runInBand --coverage",
        "test:watch": "jest -c ./jest-unit.config.ts --silent --noStackTrace --runInBand --watchAll",
        "migration:generate:test": "cross-env API_ENV=test npm run typeorm migration:generate -- -d ./src/main/config/typeorm.config.ts tests/app/shared/database/migrations/TestsMigration",
        "migration:run:test": "cross-env API_ENV=test npm run typeorm migration:run -- -d ./src/main/config/typeorm.config.ts",
        "test:integration": "cross-env API_ENV=test jest -c ./jest-integration.config.ts --silent --noStackTrace --runInBand",
        "test:integration:verbose": "cross-env API_ENV=test jest -c ./jest-integration.config.ts --verbose --runInBand --detectOpenHandles",
        "test:integration:coverage": "cross-env API_ENV=test jest -c ./jest-integration.config.ts --runInBand --coverage",
        "test:integration:watch": "cross-env API_ENV=test jest -c ./jest-integration.config.ts --silent --noStackTrace --runInBand --watchAll",
        "test:setup": "rimraf database.sqlite3 && rimraf ./tests/app/shared/database/migrations && npm run migration:generate:test && npm run migration:run:test"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "cpf-cnpj-validator": "^1.0.3",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "ioredis": "^5.3.2",
        "jest": "^29.5.0",
        "pg": "^8.10.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^5.0.0",
        "sqlite3": "^5.1.6",
        "supertest": "^6.3.3",
        "typeorm": "^0.3.11",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.15",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.11.18",
        "@types/pg": "^8.6.6",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^9.0.0",
        "ts-jest": "^29.1.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.4"
    }
}
